{
    "name": "crosiersource/crosierlib-radx",
    "description": "Bundle para o CrosierApp Radx",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Eduardo Pauluk",
            "email": "carlospauluk@gmail.com"
        }
    ],
    "require": {
        "crosiersource/crosierlib-base": "^1.15.0",
        "vich/uploader-bundle": "^1.13",
        "nfephp-org/sped-common": "^5.1",
        "nfephp-org/sped-da": "^1.1",
        "nfephp-org/sped-nfe": "^5.1",
        "nfephp-org/sped-cte": "^5.0",
        "econea/nusoap": "^0.9.7",
        "ext-bcmath": "*",
        "ext-simplexml": "*"
    },
    "config": {
        "minimum-stability": "dev",
        "prefer-stable": false,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "CrosierSource\\CrosierLibRadxBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
