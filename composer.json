{
  "name": "crosiersource/crosierlib-radx",
  "description": "Bundle para o CrosierApp Radx",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Eduardo Pauluk",
      "email": "carlospauluk@gmail.com"
    }
  ],
  "require": {
    "php": "7.4.*",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-zip": "*",
    "php-http/guzzle6-adapter": "^1.1",
    "symfony/cache": "5.0.*",
    "symfony/console": "5.0.*",
    "symfony/debug-bundle": "5.0.*",
    "symfony/form": "5.0.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/orm-pack": "1.*",
    "symfony/security-bundle": "5.0.*",
    "symfony/serializer-pack": "1.*",
    "symfony/validator": "5.0.*",
    "twig/extensions": "^1.5",
    "vich/uploader-bundle": "^1.13",
    "nfephp-org/sped-common": "^5.0",
    "nfephp-org/sped-da": "^0",
    "nfephp-org/sped-nfe": "^5.0",
    "scienta/doctrine-json-functions": "~4.1",
    "crosiersource/crosierlib-base": "@dev"
  },
  "autoload": {
    "psr-4": {
      "CrosierSource\\CrosierLibRadxBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../crosierlib-base/",
      "options": {
        "symlink": true
      }
    }
  ]
}
