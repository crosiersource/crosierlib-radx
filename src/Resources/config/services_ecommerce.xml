<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

  <services>

    <!-- IntegradorEcommerceFactory -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_ecommerce_factory"
             class="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorEcommerceFactory">
      <argument type="service_locator">
        <argument key="WEBSTORM" type="service"
                  id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_web_storm"/>
        <argument key="TRAY" type="service"
                  id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_tray"/>
        <argument key="SIMPLO7" type="service"
                  id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_simplo7"/>
        <argument key="SIMPLO7" type="service"
                  id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_mercado_pago"/>
      </argument>
      <argument type="service" id="doctrine.orm.default_entity_manager"/>
    </service>
    <service id="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorEcommerceFactory"
             alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_ecommerce_factory"/>


    <!-- IntegrarProdutoEcommerceHandler -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrar_produto_ecommerce_handler"
             class="CrosierSource\CrosierLibRadxBundle\Messenger\Ecommerce\MessageHandler\IntegrarProdutoEcommerceHandler">
      <tag name="messenger.message_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
      <argument type="service" id="doctrine.orm.default_entity_manager"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_ecommerce_factory"/>
    </service>
    <service
      id="CrosierSource\CrosierLibRadxBundle\Messenger\Ecommerce\MessageHandler\IntegrarProdutoEcommerceHandler"
      alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrar_produto_ecommerce_handler"/>


    <!-- IntegradorWebStorm -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_web_storm"
             class="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorWebStorm">
      <argument type="service"
                id="crosier_source.crosier_lib_base_bundle.entity_handler.config.app_config_entity_handler"/>
      <argument type="service" id="security.helper"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.depto_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.grupo_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.subgrupo_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.produto_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_item_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.crm.cliente_entity_handler"/>
      <argument type="service" id="Vich\UploaderBundle\Templating\Helper\UploaderHelper"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="messenger.bus.default"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
    </service>
    <service id="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorWebStorm"
             alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_web_storm"/>


    <!-- IntegradorSimplo7 -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_simplo7"
             class="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorSimplo7">
      <argument type="service"
                id="crosier_source.crosier_lib_base_bundle.entity_handler.config.app_config_entity_handler"/>
      <argument type="service" id="security.helper"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.produto_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_item_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.business.fiscal.nota_fiscal_business"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.business.vendas.venda_business"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.crm.cliente_entity_handler"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="messenger.bus.default"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_mercado_pago"/>
    </service>
    <service id="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorSimplo7"
             alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_simplo7"/>


    <!-- IntegradorMercadoPago -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_mercado_pago"
             class="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorMercadoPago">
      <argument type="service"
                id="crosier_source.crosier_lib_base_bundle.entity_handler.config.app_config_entity_handler"/>
      <argument type="service" id="security.helper"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.produto_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.produto_preco_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_item_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_pagto_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.crm.cliente_entity_handler"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="messenger.bus.default"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
    </service>
    <service id="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorMercadoPago"
             alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_mercado_pago"/>


    <!-- IntegradorTray -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_tray"
             class="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorTray">
      <argument type="service" id="security.helper"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.depto_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.grupo_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.subgrupo_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.produto_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_item_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.business.fiscal.nota_fiscal_business"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.crm.cliente_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.business.vendas.venda_business"/>
    </service>
    <service id="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorTray"
             alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_tray"/>

    <!-- IntegradorMercadoLivre -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_mercado_livre"
             class="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorMercadoLivre">
      <argument type="service" id="security.helper"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.depto_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.estoque.produto_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_entity_handler"/>
      <argument type="service"
                id="crosier_source.crosier_lib_radx_bundle.entity_handler.vendas.venda_item_entity_handler"/>
    </service>
    <service id="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\IntegradorMercadoLivre"
             alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_mercado_livre"/>


    <!-- ClienteConfigEntityHandler -->
    <service id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.cliente_config_entity_handler"
             class="CrosierSource\CrosierLibRadxBundle\EntityHandler\Ecommerce\ClienteConfigEntityHandler">
      <argument type="service" id="doctrine"/>
      <argument type="service" id="security.helper"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
    </service>
    <service
      id="CrosierSource\CrosierLibRadxBundle\EntityHandler\Ecommerce\ClienteConfigEntityHandler"
      alias="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.cliente_config_entity_handler" public="true"/>

    <!-- MercadoLivreItemEntityHandler -->
    <service id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.mercado_livre_item_entity_handler"
             class="CrosierSource\CrosierLibRadxBundle\EntityHandler\Ecommerce\MercadoLivreItemEntityHandler">
      <argument type="service" id="doctrine"/>
      <argument type="service" id="security.helper"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
    </service>
    <service
      id="CrosierSource\CrosierLibRadxBundle\EntityHandler\Ecommerce\MercadoLivreItemEntityHandler"
      alias="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.mercado_livre_item_entity_handler" public="true"/>

    <!-- MercadoLivrePerguntaEntityHandler -->
    <service id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.mercado_livre_pergunta_entity_handler"
             class="CrosierSource\CrosierLibRadxBundle\EntityHandler\Ecommerce\MercadoLivrePerguntaEntityHandler">
      <argument type="service" id="doctrine"/>
      <argument type="service" id="security.helper"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
    </service>
    <service
      id="CrosierSource\CrosierLibRadxBundle\EntityHandler\Ecommerce\MercadoLivrePerguntaEntityHandler"
      alias="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.mercado_livre_pergunta_entity_handler" public="true"/>


    <!-- TrayVendaEntityHandler -->
    <service id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.tray_venda_entity_handler"
             class="CrosierSource\CrosierLibRadxBundle\EntityHandler\Ecommerce\TrayVendaEntityHandler">
      <argument type="service" id="doctrine"/>
      <argument type="service" id="security.helper"/>
      <argument type="service" id="parameter_bag"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
    </service>
    <service
      id="CrosierSource\CrosierLibRadxBundle\EntityHandler\Ecommerce\TrayVendaEntityHandler"
      alias="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.tray_venda_entity_handler" public="true"/>



    <!-- MercadoLivreBusiness -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.mercado_livre_business"
             class="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\MercadoLivreBusiness">
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.cliente_config_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.mercado_livre_pergunta_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.mercado_livre_item_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_mercado_livre"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.entity_handler.config.push_message_entity_handler"/>
    </service>
    <service id="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\MercadoLivreBusiness"
             alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.mercado_livre_business"/>
    
    <!-- TrayBusiness -->
    <service id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.tray_business"
             class="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\TrayBusiness">
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.cliente_config_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.entity_handler.ecommerce.tray_venda_entity_handler"/>
      <argument type="service" id="crosier_source.crosier_lib_radx_bundle.business.ecommerce.integrador_tray"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.business.config.syslog_business"/>
      <argument type="service" id="crosier_source.crosier_lib_base_bundle.entity_handler.config.push_message_entity_handler"/>
    </service>
    <service id="CrosierSource\CrosierLibRadxBundle\Business\Ecommerce\TrayBusiness"
             alias="crosier_source.crosier_lib_radx_bundle.business.ecommerce.tray_business"/>
    
    
  </services>
</container>
